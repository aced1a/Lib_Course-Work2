<UserControl x:Class="Library.View.SelectedDictionaryMenuSecondPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Library.View"
             mc:Ignorable="d" 
             d:DesignHeight="500" d:DesignWidth="120">
    <Grid>
        <local:PathButton Margin="20,10,0,0"
                Command="{Binding OnLoadStoriesCommand}"
                Caption="Истории"
                Icon="{StaticResource StoryIcon}" HorizontalAlignment="Left"/>
        <local:PathButton Margin="10,110,0,0"
                Command="{Binding OnLoadCoverTypesCommand}"
                Caption="Переплеты"
                Icon="{StaticResource OpenedBookIcon}" HorizontalAlignment="Left"/>
        <local:PathButton Margin="15,210,0,0"
                Command="{Binding OnLoadBindingTypesCommand}"
                Caption="Обложки"
                Icon="{StaticResource OpenedBookIcon}" HorizontalAlignment="Left"/>
        <local:PathButton Margin="10,310,0,0"
                Command="{Binding OnLoadSettingsCommand}"
                Caption="Настройки"
                Icon="{StaticResource SettingsIcon}" HorizontalAlignment="Left"/>
        <local:PathButton Margin="35,410,0,0"
                Command="{Binding OnLoadPreviusPageCommand}"
                
                Icon="{StaticResource LeftArrowIcon}" HorizontalAlignment="Left"/>
    </Grid>
</UserControl>
